@using LearnEXM.Foundation.Extensions.Extensions.AssetsHelper
@model LearnEXM.Feature.SitecoreCinema.Models.WhatWeKnowViewModel

<aside>
  <div id="tree"></div>
  @if (Model?.WhatWeKnowTree != null)
  {
    <div id="what-we-know-data-island" style="display:none">
      @Html.Raw(Model.WhatWeKnowTree.TreeWriter.DrawDataForTree())
    </div>

  }
  else
  {
    <h3>Null Model</h3>
  }
</aside>